{
  "name": "ms-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=127.0.0.1",
    "start-mobile": "ng serve --configuration=mobile",
    "build": "ng build --configuration production --output-hashing=none --deploy-url=public/static/angular/ --aot=true  --optimization",
    "build:stats": "ng build --configuration production --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/ms-front-end/stats-es2015.json",
    "dev": "ng build --vendorChunk=false --output-hashing=none --outputPath=../ms_back_end/static/angular --baseHref=http://localhost:8000/static/angular/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/localize": "^15.2.9",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "angular-resize-event": "^3.2.0",
    "ckeditor5-build-rescodex": "^1.0.10",
    "github-markdown-css": "^5.2.0",
    "ng-recaptcha": "^10.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.25",
    "codelyzer": "^6.0.2",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}